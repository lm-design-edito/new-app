    "start-server:prod": "NODE_ENV=production node server/dist/app.js",
    "dev": "concurrently -n 'build-src,build-srv,serve    ' 'npm run build-source:dev' 'npm run build-server:dev' 'npm run start-server:dev'",
    "prod": "npm run build-source:prod && npm run build-server:prod && npm run start-server:prod",
    "start": "npm run dev",
    "predeploy": "npm run build-source:prod",

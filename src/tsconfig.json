{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "esbuild-style-plugin"
    ],
    "lib": [
      "DOM",
      "DOM.Iterable"
    ]
  }
}
